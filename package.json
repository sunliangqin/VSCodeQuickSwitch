{
    "name": "quick-switch",
    "displayName": "Quick Switch",
    "description": "Enables quick switch among relevant files",
    "version": "1.8.0",
    "license": "MIT",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "url": "https://github.com/sunliangqin/VSCodeQuickSwitch.git"
    },
    "publisher": "Liangqin",
    "activationEvents": [
        "onCommand:quickSwitch.switchFile"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "quickSwitch.switchFile",
                "title": "Switch to File"
            }
        ],
        "keybindings": [
            {
                "command": "quickSwitch.switchFile",
                "key": "ctrl+alt+s",
                "when": "editorTextFocus"
            }
        ],
        "configuration": [
            {
                "title": "Quick Switch",
                "properties": {
                    "quickSwitch.rules": {
                        "type": "array",
                        "default": [],
                        "items": {
                            "type": "object",
                            "properties": {
                                "pattern": {
                                    "type": "string"
                                },
                                "list": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        },
                        "description": "Quick switch rules"
                    },
                    "quickSwitch.fallbackToQuickOpen": {
                        "type": "boolean",
                        "default": "true"
                    },
                    "quickSwitch.quickOpenKeyword": {
                        "type": "object",
                        "properties": {
                            "pattern": {
                                "type": "string"
                            },
                            "result": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.17.27",
        "@types/vscode": "^1.47.0",
        "tslint": "^5.20.1",
        "typescript": "^3.9.7"
    }
}
