{
    "name": "quick-switch",
    "displayName": "Quick Switch",
    "description": "Enables quick switch among relevant files",
    "version": "1.2.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "url": "https://github.com/sunliangqin/VSCodeQuickSwitch.git"
    },
    "publisher": "Liangqin",
    "activationEvents": [
        "onCommand:extension.switchFile"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.switchFile",
                "title": "Switch to file"
            }
        ],
        "keybindings": [
            {
                "command": "extension.switchFile",
                "key": "ctrl+alt+s",
                "when": "editorTextFocus"
            }
        ],
        "configuration": [
            {
                "title": "Quick Switch",
                "properties": {
                    "quickSwitch.fileGroups": {
                        "type": "array",
                        "default": [],
                        "items": {
                            "type": "object",
                            "properties": {
                                "pattern": {
                                    "type": "string"
                                },
                                "list": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        },
                        "description": "Quick switch file groups"
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.17.24",
        "@types/vscode": "^1.36.0",
        "tslint": "^5.20.1",
        "typescript": "^3.9.3"
    }
}
